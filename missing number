#include<stdio.h>
int main()
{
    int n,j,i;
    printf("enter n value\n");
    scanf("%d",&n);
    int arr[n-1];
    int sum=0,ms;
    printf("array is");
    for(i=0;i<=n;i++)
    {
        scanf("%d",&arr[i]);
        sum=sum+arr[i];
    }
    ms=(n*(n+1))/2-sum;
    printf("missing number=%d",ms);
}
